<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Route.
    -->
    <changeSet id="20181201110719-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="vehicle_id"
                                 baseTableName="route"
                                 constraintName="fk_route_vehicle_id"
                                 referencedColumnNames="id"
                                 referencedTableName="vehicle"/>

        <addForeignKeyConstraint baseColumnNames="factory_id"
                                 baseTableName="route"
                                 constraintName="fk_route_factory_id"
                                 referencedColumnNames="id"
                                 referencedTableName="factory"/>

    </changeSet>
</databaseChangeLog>
